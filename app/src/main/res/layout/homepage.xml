            android:fontFamily="@font/bubblegum_sans"
            android:text="Children"
            android:textColor="#2CA6F0"
            android:textSize="23dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/osochildrennombre"
            android:layout_width="93dp"
            android:layout_height="31dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="56dp"
            android:fontFamily="@font/bubblegum_sans"
            android:text="Andree"
            android:textColor="#3BBAFD"
            android:textSize="30dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/osomatterhistorial"
            android:layout_width="93dp"
            android:layout_height="31dp"
            android:layout_marginStart="140dp"
            android:layout_marginTop="56dp"
            android:fontFamily="@font/bubblegum_sans"
            android:text="Historial"
            android:textColor="#3BBAFD"
            android:textSize="30dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/osohoras"
            android:layout_width="58dp"
            android:layout_height="32dp"
            android:layout_marginStart="268dp"
            android:layout_marginTop="56dp"
            android:fontFamily="@font/bubblegum_sans"
            android:text="23h"
            android:textColor="#3BBAFD"
            android:textSize="30dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/osomatter"
            android:layout_width="70dp"
            android:layout_height="31dp"
            android:layout_marginStart="40dp"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/bubblegum_sans"
            android:text="Matter"
            android:textColor="#2CA6F0"
            android:textSize="23dp"
            app:layout_constraintStart_toEndOf="@+id/osochildren"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/osohours"
            android:layout_width="93dp"
            android:layout_height="31dp"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/bubblegum_sans"
            android:text="total hours"
            android:textColor="#2CA6F0"
            android:textSize="23dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.571"
            app:layout_constraintStart_toEndOf="@+id/osomatter"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/usuario1"
        android:layout_width="167dp"
        android:layout_height="224dp"
        android:layout_marginBottom="112dp"
        android:background="@drawable/contenedoresdeusuario"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.131"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="16dp"
            android:fontFamily="@font/bubblegum_sans"
            android:text="Andree"
            android:textSize="25dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/location1"
            android:layout_width="110dp"
            android:layout_height="36dp"
            android:layout_marginStart="28dp"
            android:background="@drawable/buttonusurio"
            android:fontFamily="@font/bubblegum_sans"
            android:text="Location"
            android:textColor="#ffffff"
            android:textSize="15dp"

            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.424" />

        <Button
            android:id="@+id/location"
            android:layout_width="122dp"
            android:layout_height="36dp"
            android:background="@drawable/buttonusurio"
            android:fontFamily="@font/bubblegum_sans"
            android:text="Navegation"
            android:textColor="#ffffff"
            android:textSize="15dp"

            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.654" />

        <Button
            android:id="@+id/activitis1"
            android:layout_width="103dp"
            android:layout_height="35dp"
            android:background="@drawable/buttonusurio"
            android:fontFamily="@font/bubblegum_sans"
            android:text="Activitis"
            android:textColor="#ffffff"
            android:textSize="15dp"

            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.869" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="60dp"
            android:layout_height="52dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@android:drawable/ic_menu_gallery" />
